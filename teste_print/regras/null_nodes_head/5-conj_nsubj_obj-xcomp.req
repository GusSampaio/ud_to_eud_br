pattern{
  e:CONJ1 -[conj]-> SUBJ2;
  e1:CONJ1 -[1=nsubj]-> SUBJ1;  SUBJ1 << CONJ1; CONJ1[DVerbForm];
  CONJ1 -[obj|xcomp]-> ARG1; CONJ1 << ARG1; ARG1 << SUBJ2;
  SUBJ2[Dupos=NOUN|NUM|PRON|PROPN|X];
  SUBJ2 -[orphan]-> OBJ2; OBJ2[Dupos=NOUN|PRON|PROPN|X]; SUBJ2 << OBJ2}
without{SUBJ2 -[case]-> *}
without{* -[^conj]-> SUBJ2}
